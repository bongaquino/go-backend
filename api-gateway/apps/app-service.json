{
  "name": "app-service",
  "api_id": "app-service",
  "org_id": "default",
  "use_keyless": true,
  "proxy": {
    "listen_path": "/app/iam",
    "target_url": "http://iam-service:3000",
    "strip_listen_path": true
  },
  "custom_middleware": {
    "pre": [
      {
        "name": "authentication",
        "path": "/opt/tyk-gateway/middleware/authentication.js",
        "require_session": true,
        "raw_body_only": false
      }
    ]
  }
}
