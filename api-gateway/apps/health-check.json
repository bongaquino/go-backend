{
  "name": "health-check",
  "api_id": "health-check",
  "org_id": "default",
  "use_keyless": true,
  "proxy": {
    "listen_path": "/health",
    "target_url": "http://localhost",
    "strip_listen_path": true
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "default": {
        "name": "default",
        "use_extended_paths": true,
        "extended_paths": {
          "virtual": [
            {
              "path": "/health",
              "method": "GET",
              "script_name": "checkHealth"
            }
          ]
        }
      }
    }
  }
}