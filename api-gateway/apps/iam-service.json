{
  "name": "iam-service",
  "api_id": "iam-service",
  "org_id": "default",
  "use_keyless": true,
  "proxy": {
    "listen_path": "/iam",
    "target_url": "http://iam-service:3000",
    "strip_listen_path": true
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "default": {
        "name": "default",
        "use_extended_paths": true
      }
    }
  },
  "custom_middleware": {
    "pre": [
      {
        "name": "authentication",
        "path": "/opt/tyk-gateway/middleware/authentication.js",
        "require_session": true,
        "raw_body_only": false
      }
    ]
  }
}
